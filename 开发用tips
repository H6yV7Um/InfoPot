$.when(self.getRecentTransactions(),self.getBill()).then(function(recentTransactionsInfo,billInfo){
                
},function( errorCode ){

});


getRecentTransactions:function(){
    var deferred = Deferred();
    异步function onAlways(data) {
    	deferred.resolve(data.recentTransactionsInfo);
    }
    return deferred.promise();
},
getBill:function(){
    var deferred = Deferred();
    异步function onAlways(data) {
                deferred.resolve(data.billInfo || {});
                deferred.resolve({showCopyBtn:Lizard.app.vendor.is('CTRIP'),hasBill:false,installable:false});
                deferred.reject('PORTALSCQA0001');
                deferred.reject();
        }
    }
    return deferred.promise();
},

